{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "project",
  "projects": {
    "fusion": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "ws",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/www/fusion",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/favicon.png",
              "src/fusion-assets",
              {
                "glob": "**/*.*",
                "input": "./node_modules/@aastrika_npmjs/discussions-ui-v8/assets",
                "output": "./assets/discussion-ui"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles"
              ]
            },
            "styles": [
              {
                "input": "src/styles/libraries.scss",
                "bundleName": "libraries"
              },
              "node_modules/material-icons/iconfont/material-icons.scss",
              "src/styles/mat-typography.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/styles/styles.scss",
              "node_modules/@fortawesome/fontawesome-free/css/all.css",
              "node_modules/shepherd.js/dist/css/shepherd.css",
              "node_modules/@aastrika_npmjs/discussions-ui-v8/assets/styles/global.scss",
              {
                "input": "src/themes/theme-deep-purple.scss",
                "bundleName": "theme-deep-purple",
                "lazy": true
              },
              {
                "input": "src/themes/theme-grey.scss",
                "bundleName": "theme-grey",
                "lazy": true
              },
              {
                "input": "src/themes/theme-indigo.scss",
                "bundleName": "theme-indigo",
                "lazy": true
              },
              {
                "input": "src/themes/theme-lite-blue.scss",
                "bundleName": "theme-lite-blue",
                "lazy": true
              },
              {
                "input": "src/themes/theme-orange.scss",
                "bundleName": "theme-orange",
                "lazy": true
              },
              {
                "input": "src/themes/theme-teal.scss",
                "bundleName": "theme-teal",
                "lazy": true
              },
              {
                "input": "src/themes/theme-blue.scss",
                "bundleName": "theme-blue",
                "lazy": true
              },
              {
                "input": "src/themes/theme-deep-blue.scss",
                "bundleName": "theme-deep-blue",
                "lazy": true
              },
              {
                "input": "src/themes/theme-igot.scss",
                "bundleName": "theme-igot",
                "lazy": true
              },
              "node_modules/pdfjs-dist/web/pdf_viewer.css",
              "src/fusion-assets/fonts/poppins/stylesheet.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "src/fusion-assets/telemetry.min.js",
              "node_modules/shepherd.js/dist/js/shepherd.min.js",
              "node_modules/jspdf-autotable/dist/jspdf.plugin.autotable.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "11mb",
                  "maximumError": "11mb"
                }
              ],
              "serviceWorker": false,
              "ngswConfigPath": "ngsw-config.json"
            },
            "hi":{
                "aot": true,
                "i18nFile": "locale/messages.hi.xlf",
                "i18nFormat": "xlf",
                "i18nLocale": "hi",
                "i18nMissingTranslation": "warn"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "port": 3000,
            "browserTarget": "fusion:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "fusion:build:production"
            },
            "hi": {
              "browserTarget": "fusion:build:hi"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "fusion:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/fusion-assets",
              {
                "glob": "**/*.*",
                "input": "./node_modules/@aastrika_npmjs/discussions-ui-v8/assets",
                "output": "./assets/discussion-ui"
              }
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "app/project/ws/app/src/lib/routes/app-toc/components/app-toc-content-card/app-toc-content-card.component",
              "project/ws/viewer/src/lib/plugins/quiz/quiz.component"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "fusion:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "fusion:serve:production"
            }
          }
        }
      }
    },
    "@ws-widget/resolver": {
      "projectType": "library",
